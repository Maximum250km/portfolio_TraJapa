<div class="uk-container uk-width-large uk-background-muted uk-margin-medium">
	<fieldset class="uk-fieldset uk-margin-medium">
		<legend class="uk-legend uk-text-center uk-text-primary">Log In</legend>
			<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
	 			<div class="field">
	 				<%= f.label :Email, class:"uk-label" %><br />
	 				<%= f.email_field :email, autofocus: true, autocomplete: "email" %>
	 			</div>
	 			<div class="field">
	 				<%= f.label :password,class:"uk-label" %><br />
	 				<%= f.password_field :password, autocomplete: "current-password" %>
	 			</div>
			 	<% if devise_mapping.rememberable? %>
	 			<div class="field">
	 				<%= f.check_box :remember_me,class:"uk-label" %>
	 				<%= f.label :remember_me %>
	 			</div>
	 		<% end %>
			<div class="actions">
	 			<%= f.submit "Log in" %>
	 		</div>
	 		<% end %>
	</fieldset>
</div>
  <%= render "devise/shared/links" %>

